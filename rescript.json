{
  "name": "dotmatrix-fileprinter",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.js",
  "dependencies": [
    "@rescript/core"
  ],
  "compiler-flags": [
    "-open RescriptCore"
  ],
  "warnings": {
    "error": "+101"
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  }
}
